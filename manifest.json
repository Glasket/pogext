{
  "name": "True PogChamp for Twitch",
  "version": "1.0",
  "description": "Replaces the Twitch Global PogChamp with the original (runs locally using Javascript).",
  "manifest_version": 2,
  "permissions": ["https://*.twitch.tv/*", "activeTab"],
  "content_scripts": [{
      "matches": ["https://*.twitch.tv/"],
      "js": ["pog.js"]
  }],
  "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "64": "images/64.png",
      "128": "images/128.png"
  }
}
